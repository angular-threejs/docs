{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@ui/*": ["./src/components/ui/*"],
			"@scenes/*": ["./src/components/scenes/*"],
			"@soba/*": ["./src/components/soba/*"],
			"@postprocessing/*": ["./src/components/postprocessing/*"],
			"@common-assets/*": ["./src/components/common-assets/*"]
		}
	}
}
